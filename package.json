{
  "name": "workstation",
  "version": "0.0.0-development",
  "description": "A workstation that lives in the browser",
  "main": "index.js",
  "author": "Jae Bradley <jae.b.bradley@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "NODE_ENV=production BABEL_ENV=production ./node_modules/.bin/webpack --config=webpack.production.config.js",
    "deploy:gh-pages": "npm run build && gh-pages -d dist",
    "is-es5": "es-check es5 ./dist/*.js",
    "lint": "eslint --ext .js --ext .jsx .",
    "precommit": "npm run lint",
    "prepublishOnly": "npm run build",
    "start": "NODE_ENV=development BABEL_ENV=development node_modules/.bin/webpack-dev-server --config=webpack.development.config.js --progress",
    "debug-test": "node --inspect-brk node_modules/.bin/jest --runInBand --coverage",
    "test": "jest --coverage --passWithNoTests"
  },
  "dependencies": {
    "@material-ui/core": "^1.3.1",
    "@material-ui/icons": "^1.1.0",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.0",
    "redux-actions": "^2.4.0",
    "redux-devtools-extension": "^2.13.5",
    "redux-middleware-fsa-validator": "^1.0.0",
    "redux-saga": "^0.16.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.53",
    "@babel/core": "^7.0.0-beta.53",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.53",
    "@babel/plugin-proposal-decorators": "^7.0.0-beta.53",
    "@babel/preset-env": "^7.0.0-beta.53",
    "@babel/preset-react": "^7.0.0-beta.53",
    "@commitlint/config-angular": "^7.0.1",
    "@commitlint/prompt": "^7.0.0",
    "@commitlint/prompt-cli": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^8.2.6",
    "babel-jest": "^23.4.0",
    "babel-loader": "^8.0.0-beta",
    "classnames": "^2.2.6",
    "codecov": "^3.0.4",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-import-resolver-webpack": "^0.10.1",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.0",
    "eslint-plugin-react": "^7.10.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^0.14.3",
    "jest": "^23.4.0",
    "react-dev-utils": "^5.0.1",
    "redux-mock-store": "^1.5.3",
    "semantic-release": "^15.7.1",
    "travis-deploy-once": "^5.0.1",
    "webpack": "^4.x.x",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4",
    "webpack-merge": "^4.1.3"
  },
  "jest": {
    "moduleNameMapper": {
      "^Todos(.*)$": "<rootDir>/src/todos$1"
    }
  }
}
